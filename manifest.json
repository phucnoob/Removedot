{
  "manifest_version": 3,
  "name": "Remove dot bracket",
  "description": "Remove dot bracket like (.), [.].. in urls",
  "version": "1.0",

  "content_scripts": [
    {
        "matches": ["<all_urls>" ],
        "js": ["content-script.js"]
    }
  ],

//   "action": {
//     "default_title": "Remove bracket",
//     "default_popup": "index.html"
//   },
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  }
}